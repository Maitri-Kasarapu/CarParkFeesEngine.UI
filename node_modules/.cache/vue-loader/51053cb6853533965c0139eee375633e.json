me-picker\n                v-else\n                :value=\"value\"\n                @input=\"onInput\"\n            />\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: 'FieldPicker',\n\n        props: {\n            'type': {\n                type: String,\n                default: 'date',\n                validator: (type) => {\n                    const VALID_TYPES = [ 'date', 'time' ]\n\n                    return VALID_TYPES.includes(type)\n                }\n            },\n            'label': {\n                type: String,\n                default: 'Select'\n            },\n            'value': {\n                type: String\n            }\n        },\n\n        data () {\n            return {\n                showPicker: false\n            }\n        },\n\n        methods: {\n            onClick () {\n                this.showPicker = true\n            },\n\n            onInput (value) {\n                this.$emit('input', value)\n            },\n\n            onClosePicker () {\n                // To run after the \"onClick\" method\n                setTimeout(() => {\n                    this.showPicker = false\n                }, 0)\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .field-picker {\n        position: relative;\n        width: 100%;\n    }\n\n    .field-picker__popup {\n        position: absolute;\n        right: 0;\n        top: 0;\n        z-index: 1;\n    }\n\n    .field-picker__popup--close {\n        position: absolute;\n        top: 8px;\n        right: 12px;\n        z-index: 2;\n        color: white;\n        cursor: pointer;\n    }\n</style>"]}]}